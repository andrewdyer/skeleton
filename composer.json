{
    "name": "andrewdyer/skeleton",
    "description": "A starter structure built with the Slim PHP framework",
    "keywords": ["framework", "slim"],
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0",
        "fakerphp/faker": "^1.16",
        "illuminate/database": "^8.61",
        "league/tactician": "^1.1",
        "monolog/monolog": "^2.3",
        "robmorgan/phinx": "^0.12.8",
        "slim/slim": "^3.12",
        "slim/twig-view": "^2.5",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^5.3"
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
